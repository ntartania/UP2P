package protocol.com.dan.jtella;

import java.util.EventObject;

/**
 * Passed to a ConnectedHostsListener when the list of connected hosts changes.
 */
public class HostsChangedEvent extends EventObject {

	/**
	 * Constructor
	 * 
	 * @param source The Object that generated the Event
	 */
	public HostsChangedEvent(Object source) {
		super(source);
	}
}
